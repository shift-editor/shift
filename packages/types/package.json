{
  "name": "@shift/types",
  "version": "0.0.1",
  "private": true,
  "description": "Shared TypeScript types for Shift font editor",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./generated": {
      "types": "./src/generated/index.ts",
      "import": "./src/generated/index.ts"
    }
  },
  "scripts": {
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@shift/tsconfig": "workspace:*",
    "typescript": "^5.3.0"
  }
}
